<h1>Listing images</h1>

<div class="row">
  <div class="col-md-8">



      <div class="image-box">
        <div class="image-box-inside">
            <% @images.each do |image| %>
              <% if image.user_id == current_user.id %>




                    <%= link_to image do %>
                      <%= image_tag image.photo.url(:thumb) %>
                    <%end%>

              <% end %>
            <%end%>
          </div>
      </div>
  </div>


  <div class="col-md-3">What</div>

</div>
<br>

<div class="upload">
  <%= link_to 'Upload Image', new_image_path %>

</div>
